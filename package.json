{
  "name": "projeto_app_unike",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "flow": "flow",
    "flow-stop": "flow stop",
    "precommit": "lint-staged && yarn test --passWithNoTests",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test:unit": "jest",
    "test": "yarn lint && yarn flow && jest",
    "enable-kvm": "sudo chown reactnative:reactnative -R /dev/kvm",
    "device": "npm run enable-kvm && emulator -avd Pixel_2_API_28",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint src",
    "pretty": "prettier --write \"src/**/*.js\"",
    "lint-staged": "lint-staged",
    "debugger": "react-devtools",
    "log-android": "react-native log-android"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.4.2",
    "Base64": "^1.0.2",
    "axios": "^0.19.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "native-base": "^2.12.1",
    "react": "16.8.3",
    "react-native": "0.59.8",
    "react-native-datepicker": "^1.7.2",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-text-input-mask": "^1.0.4",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^3.11.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.16.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "flow-bin": "0.92.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "metro-react-native-babel-preset": "^0.54.0",
    "prettier": "^1.17.1",
    "react-devtools": "^3.6.1",
    "react-dom": "^16.8.6",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)test.js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  }
}
